def majorityElement(nums):
    val = 0
    c = 0
    for num in nums:
        if c == 0:
            val = num
        if num == val:
            c += 1
        else:
            c -= 1
    return val
nums = list(map(int, input("Enter numbers separated by space: ").split()))
print("Majority Element:", majorityElement(nums))
